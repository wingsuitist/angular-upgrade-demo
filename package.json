{
  "name": "angular-upgrade-demo",
  "author": "Craig Shoemaker <craig@craigshoemaker.net> (http://craigshoemaker.net)",
  "description": "Upgrade/migrate Angular 1 apps to Angular 2",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "typings-install": "npm run typings install dt~jquery dt~angular dt~angular-route -- --save --global"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/craigshoemaker/angular-upgrade-demo.git"
  },
    "dependencies": {
    "@angular/common": "2.4.3",
    "@angular/compiler": "2.4.3",
    "@angular/core": "2.4.3",
    "@angular/http": "2.4.3",
    "@angular/platform-browser": "2.4.3",
    "@angular/platform-browser-dynamic": "2.4.3",
    "@angular/upgrade": "2.4.3",
    "angular": "^1.5.6",
    "angular-resource": "^1.5.7",
    "angular-route": "^1.5.7",
    "angular2-in-memory-web-api": "0.0.21",
    "bootstrap": "^3.3.6",
    "es6-shim": "^0.35.2",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.1",
    "systemjs": "0.19.41",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "lite-server": "^2.2.2",
    "typescript": "~2.1.5",
    "typings": "^2.1.0"
  }
}